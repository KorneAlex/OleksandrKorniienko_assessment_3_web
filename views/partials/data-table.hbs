<table class="table is-fullwidth">
<thead>
    <tr>
        <th>Code</th>
        <th>Temperature</th>
        <th>Wind Speed</th>
        <th>Wind Direction</th>
        <th>Pressure</th>
        <th>ID</th>
        <th>Timestamp</th>
    </tr>
</thead>
{{#each dataStore}}
  <tr>
    <td>{{record.code}}</td>
    <td>{{record.temperature}}</td>
    <td>{{record.wind_speed}}</td>
    <td>{{record.wind_direction}}</td>
    <td>{{record.pressure}}</td>
    <td>{{record.id}}</td>
    <td>{{record.timestamp}}</td>
  </tr>
{{/each}}
</table>