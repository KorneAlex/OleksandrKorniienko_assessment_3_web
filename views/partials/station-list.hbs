<div class="header"><div style="position: relative; text-align: center;"><div class="title is-4">Station list</div></div></div>
<table class="table is-fullwidth">
<thead>
    <tr>
        <th>Name</th>
        <th>City</th>
        <th>County</th>
        <th>Latitude</th>
        <th>Longitude</th>
        <th>ID</th>
        <th>Added</th>
    </tr>
</thead>
{{#each activeStationsData}}
  <tr>
    <td>{{name}}</td>
    <td>{{city}}</td>
    <td>{{county}}</td>
    <td>{{latitude}}</td>
    <td>{{longitude}}</td>
    <td>{{id}}</td>
    <td>{{timestamp_created}}</td>
     <td><a href="/stations/{{id}}">Edit</a></td>
     <td><a href="/stations/{{id}}/deleteStation">Delete</a></td>
  </tr>
{{/each}}
</table>